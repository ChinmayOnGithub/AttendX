    android:padding="16dp">

    <!-- EditText for search -->
    <EditText
        android:id="@+id/edit_text_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Enter Course Name" />

</LinearLayout>
